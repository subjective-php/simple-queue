{
    "name": "subjective-php/simple-queue",
    "description": "The purpose of this library is to provide a set of common interfaces for simple queue implementations.",
    "license": "MIT",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "chadicus/coding-standard": "^1.6",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^6.1"
    },
    "config" : {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SubjectivePHP\\SimpleQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SubjectivePHPTest\\SimpleQueue\\": "tests"
        }
    }
}
